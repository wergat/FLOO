{
  "name": "FLOO",
  "version": "0.6.3-beta",
  "private": true,
  "description": "Force Lead Organizing OwOrlay",
  "author": "Wergat",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && tsc",
    "pack": "tsc && electron-packager . FLOO --platform win32 --arch x64 --out out/ --overwrite --ignore=\"^/src|^/public\" --asar.unpack=\"*.+(node|dll)\" --asar.unpackDir=\"img\" --icon=./img/icon.ico",
    "make": "electron-installer-windows --config installer-config.json",
    "lint": "vue-cli-service lint",
    "start": "tsc && electron .",
    "full": "npm run build && npm run pack && npm run make"
  },
  "main": "dist/main.js",
  "dependencies": {
    "@types/color": "^3.0.1",
    "buefy": "^0.9.4",
    "color": "^3.1.3",
    "core-js": "^3.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1",
    "iohook": "^0.6.6",
    "is-electron": "^2.2.0",
    "tslib": "^2.0.3",
    "vue": "^2.6.12",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "electron": "8.5.2",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.1.0",
    "electron-rebuild": "^2.0.3",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "@vue/typescript/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {}
  },
  "browserslist": [
    "Electron 8.5.2"
  ],
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "^10.1.2"
  },
  "iohook": {
    "targets": [
      "node-64",
      "electron-76"
    ],
    "platforms": [
      "win32"
    ],
    "arches": [
      "x64"
    ]
  },
  "keywords": [],
  "license": "MIT License",
  "repository": "https://github.com/wergat/FLOO"
}
